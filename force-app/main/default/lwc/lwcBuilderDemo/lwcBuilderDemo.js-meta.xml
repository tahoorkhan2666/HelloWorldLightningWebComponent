<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
	<apiVersion>52.0</apiVersion>
	<isExposed>true</isExposed>
	<masterLabel>Lwc Builder Demo</masterLabel>
	<targets>
		<target>lightning__AppPage</target>
		<target>lightning__HomePage</target>
		<target>lightning__RecordPage</target>
		<target>lightning__FlowScreen</target>
		<target>lightning__Tab</target>
	</targets>
	<targetConfigs>
		<targetConfig targets="lightning__AppPage">
			<property name="componentTitle" type="String" default="" label="Component title" required="true" />
			<supportedFormFactors>
				<supportedFormFactor type="Small"/>
				<supportedFormFactor type="Large"/>
			</supportedFormFactors>
		</targetConfig>
		<targetConfig targets="lightning__HomePage">
			<property name="componentTitle" type="String" default="" label="Component title" required="true" />
			<supportedFormFactors>
				<supportedFormFactor type="Large"/>
			</supportedFormFactors>
		</targetConfig>
		<targetConfig targets="lightning__RecordPage">
			<property name="recordId" type="String" default="" label="Record id" placeholder="Record Id" required="true" />
			<property name="componentTitle" type="String" default="" label="Component title" required="true" />
			<objects>
				<object>Account,Opportunity</object>
			</objects>
			<supportedFormFactors>
				<supportedFormFactor type="Small"/>
				<supportedFormFactor type="Large"/>
			</supportedFormFactors>
		</targetConfig>
		<targetConfig targets="lightning__FlowScreen">
			<property name="componentTitle" type="String" default="" label="Component title" required="true" />
		</targetConfig>
	</targetConfigs>
</LightningComponentBundle>